{
    "table_definition": {
        "name": "funding_agencies",
        "engine": "InnoDB",
        "comment": "Funding agency data for Value Analytics.",
        "columns": [
            {
                "name": "id",
                "type": "int(11)",
                "nullable": false,
                "extra": "AUTO_INCREMENT",
                "comment": "The ID for a funding agency (assigned by XDMoD)."
            },
            {
                "name": "name",
                "type": "varchar(300)",
                "nullable": false,
                "comment": "The name of a funding agency."
            }
        ],
        "indexes": [
            {
                "name": "PRIMARY",
                "columns": [
                    "id"
                ]
            },
            {
                "name": "name",
                "columns": [
                    "name"
                ],
                "is_unique": true
            }
        ]
    },

    "destination_columns": {
        "name": "name"
    }
}
